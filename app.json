{
  "expo": {
    "name": "SLIC SHE MOBILE",
    "slug": "SLIC-SHE-Application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SHEMobileApp.png",
    "scheme": "slicsheapplication",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/SHEMobileApp.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SHEMobileApp.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.SLICSHEApplication"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/SHEMobileApp.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/SHEMobileApp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "86b10cb0-95f3-42f4-9b46-4a6215d63cd7"
      }
    }
  }
}
